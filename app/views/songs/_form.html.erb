<%= form_for @song do |f| %>
  <div>
    <%= f.label :artist %><br>
    <%= f.text_field :artist  %>
  </div>
  <div>
    <%= f.label :title %><br>
    <%= f.text_field :title  %>
  </div>
  <div>
    <%= f.label :duration %><br>
    <%= f.text_field :duration  %>
  </div>
  <div>
    <%= f.label :price %><br>
    <%= f.text_field :price  %>
  </div>
  <%= f.submit :submit %>
<% end %>